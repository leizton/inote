  "files.autoSave": "afterDelay",
  "[markdown]": {
    "editor.fontFamily": "VictorMono-semibold,MiSans-Regular",
    "editor.fontWeight": "normal",
    "editor.fontSize": 14,
    "editor.lineHeight": 1.5,
    "editor.wordWrap": "off",
    "editor.rulers": [20000],
  },
  "editor.fontFamily": "VictorMono-semibold,MiSans-Regular",
  "editor.fontSize": 15,
  "editor.lineHeight": 1.7,
  "terminal.integrated.fontFamily": "VictorMono-SemiBold",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.letterSpacing": -0.04,
  "C_Cpp.workspaceSymbols": "All",
  "C_Cpp.intelliSenseEngine": "Tag Parser",
  "C_Cpp.clang_format_style": "{ BasedOnStyle: LLVM, UseTab: Never, IndentWidth: 2, TabWidth: 2, ColumnLimit: 0, NamespaceIndentation: None, PointerAlignment: Left, AllowShortIfStatementsOnASingleLine: true, AllowShortCaseLabelsOnASingleLine: true, BreakConstructorInitializersBeforeComma: true }",
  "C_Cpp.vcFormat.newLine.beforeElse": false,
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
  "C_Cpp.enhancedColorization": "enabled",
  "C_Cpp.errorSquiggles": "disabled",
  "C_Cpp.experimentalFeatures": "enabled",
  "C_Cpp.intelliSenseCacheSize": 8192,
  "C_Cpp.intelliSenseEngineFallback": "enabled",
  "C_Cpp.intelliSenseUpdateDelay": 3000,
  "C_Cpp.loggingLevel": "Information",
  "[cpp]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    }
  },
  "[jsonc]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorWidth": 2,
  "editor.detectIndentation": false,
  "editor.dragAndDrop": false,
  "editor.foldingStrategy": "auto",
  "editor.fontLigatures": true,
  "editor.insertSpaces": true,
  "editor.largeFileOptimizations": true,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": true,
  "editor.mouseWheelScrollSensitivity": 2.0,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "none",
  "editor.roundedSelection": true,
  "editor.rulers": [
    200
  ],
  "editor.scrollbar.verticalScrollbarSize": 5,
  "editor.smoothScrolling": true,
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "emmet.showSuggestionsAsSnippets": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": false,
  "files.associations": {
    "BUILD": "python"
  },
  "files.exclude": {},
  "git.confirmSync": false,
  "git.decorations.enabled": true,
  "git.enableSmartCommit": true,
  "gitblame.statusBarMessageFormat": "${author.name}  ${committer.date}",
  "http.proxySupport": "off",
  "search.useIgnoreFiles": false,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.showExitAlert": false,
  "typescript.check.npmIsInstalled": false,
  "window.title": "${rootName}",
  "workbench.editor.enablePreview": false,
  "workbench.editor.limit.value": 110,
  "workbench.editor.showTabs": "single",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.enableExperiments": false,
  "workbench.list.horizontalScrolling": true,
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.statusBar.visible": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "markdown.preview.lineHeight": 1.7,
  "editor.tabSize": 2,
  "redhat.telemetry.enabled": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "window.commandCenter": true,
  "workbench.layoutControl.enabled": false,
  "editor.bracketPairColorization.enabled": false,
  "debug.console.fontSize": 11,
  "gitlens.statusBar.format": "${author} ${YYYY-MM-DD HH:mm} ${' via 'pullRequest}",
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.currentLine.scrollable": false,
  "gitlens.codeLens.authors.enabled": false,
  "editor.letterSpacing": -0.2,
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.statusBar.enabled": false,
  "prettier.printWidth": 110,
  "git.openRepositoryInParentFolders": "always",
  "diffEditor.wordWrap": "off",
  "C_Cpp.vcFormat.wrap.preserveBlocks": "never",
  "yaml.format.proseWrap": "never",
  "editor.renderLineHighlight": "all",
  "editor.wrappingIndent": "none",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "workbench.activityBar.location": "hidden",
  "workbench.editor.tabActionCloseVisibility": false,
  "security.promptForLocalFileProtocolHandling": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.stickyScroll.enabled": false,
  "github.copilot.editor.enableAutoCompletions": true,
  "cmake.options.statusBarVisibility": "compact",
  "terminal.integrated.tabs.enabled": false
}